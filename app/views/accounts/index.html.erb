<p id="notice"><%= notice %></p>

<h1>Please choose your account</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.user_id %></td>
        <td><%= link_to account.name, account %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

